    {
  "version": 2,
  "builds": [
    {
      "src": "prisma/generate",
      "use": "@prismicio/vercel-builds"
    },
    {
      "src": "npx prisma generate",
      "use": "@vercel/static-build"
    },
    {
      "src": "npx next build",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}